<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
<managed-bean>
		<managed-bean-name>crimeBean</managed-bean-name>
		<managed-bean-class>com.adbms.uic.CrimeBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>messageBean</managed-bean-name>
		<managed-bean-class>com.adbms.uic.MessageBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<from-view-id>/index.jsp</from-view-id>
		<navigation-case>
			<from-outcome>SuccessAdd</from-outcome>
			<to-view-id>/addCrime.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>SuccessView</from-outcome>
			<to-view-id>/viewCrime.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/addCrime.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Success</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/viewCrime.jsp</from-view-id>
		<navigation-case>
			<from-outcome>Success</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
</faces-config>
